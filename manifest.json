{
  "name": "Bubble Tools",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "An extension providing helpful tools, shortcuts, and features to aid in and save time developing apps with Bubble.io",
  "homepage_url": "https://jadedkitty.dev/bubble-tools",
  "icons": {
    "16": "icons/bubble-tools-icon-16px.png",
    "48": "icons/bubble-tools-icon-48px.png",
    "128": "icons/bubble-tools-icon-128px.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/bubble-tools-icon-19px.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "clipboardWrite",
    "contextMenus",
    "cookies",
    "notifications",
    "tabs",
    "https://bubble.io/*",
    "https://*.bubbleapps.io/*"
  ]
}